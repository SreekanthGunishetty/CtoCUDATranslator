#include <stdio.h>

 int main(int argc , char *argv [] )
{	
	int i;
	int count=0;
	kernel double p[100];
	for(i = 0;i<100;i++)
	{
		p[i]=4;
	}
	#pragma kernel_start
	count = 0;
	for (i = 3; i <= 1000; i+=2) { 
		if ( count % 2 == 0 ) {
		p[i] -= 4 / i;
		}
		else {
		p[i] += 4 / i;
		}
		count = count+1;
	}
	#pragma kernel_end
	for ( i = 3; i <= 100; i += 2) 
	{ 
	printf( "%0.2f\t", p[i]);;
	}
	return 0;
}
